<%- include('partials/header') %>
<main class="main-content">
    <h1>Leaderboards</h1>
    <table>
        <thead>
            <tr>
                <th>Rank</th>
                <th>Streak</th>
                <th>Date Achieved</th>
            </tr>
        </thead>
        <tbody>
            <% leaderboards.forEach((entry, index) => { %>
                <tr>
                    <td><%= index + 1 %></td>
                    <td><%= entry.streak %></td>
                    <td><%= entry.date.toLocaleString() %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <a href="/">Back to Home</a>
</main>
</body>
</html>



